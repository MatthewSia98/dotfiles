XF86MonBrightnessUp
    ~/.shell-scripts/dunst/brightness.sh raise
XF86MonBrightnessDown
    ~/.shell-scripts/dunst/brightness.sh lower

XF86AudioRaiseVolume
    ~/.shell-scripts/dunst/volume.sh raise

XF86AudioLowerVolume
    ~/.shell-scripts/dunst/volume.sh lower

XF86AudioMute
    ~/.shell-scripts/dunst/volume.sh mute
